(oleo.base:define-package :oleo ()
  (:use :cl)
  ;(:nicknames :prolog :pl)
  (:import/export :oleo.base :oleo.algebraic.core :oleo.embed :oleo.prolog)
  (:export
   #:oleo-mode
   )
  (:unexport
   #:oleo-base-header)
  )

(in-package :oleo)

(defmacro oleo-mode ()
  `(oleo.base:oleo-base-header (:clap)))





